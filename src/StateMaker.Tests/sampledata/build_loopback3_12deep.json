{
  "initialState": {
    "step": 0
  },
  "rules": [
    {
      "name": "advance",
      "condition": "step >= 0",
      "transformations": {
        "step": "step + 1"
      }
    },
    {
      "name": "loop back 3",
      "condition": "step >= 3",
      "transformations": {
        "step": "step -3"
      }
    }
  ],
  "config": {
    "maxDepth": 12,
    "explorationStrategy": "BreadthFirstSearch"
  }
}